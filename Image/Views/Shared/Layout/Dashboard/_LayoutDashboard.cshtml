<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.PageTitle</title>
    <!-- PACE-->
    <link rel="stylesheet" type="text/css" href="~/Dashboard/plugins/PACE/themes/blue/pace-theme-flash.css">
    <script type="text/javascript" src="~/Dashboard/plugins/PACE/pace.min.js"></script>
    <!-- Bootstrap CSS-->
    <link rel="stylesheet" type="text/css" href="~/Dashboard/plugins/bootstrap/dist/css/bootstrap.min.css">
    <!-- Fonts-->
    <link rel="stylesheet" type="text/css" href="~/Dashboard/plugins/themify-icons/themify-icons.css">
    <!-- Malihu Scrollbar-->
    <link rel="stylesheet" type="text/css" href="~/Dashboard/plugins/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.min.css">
    <!-- Animo.js-->
    <link rel="stylesheet" type="text/css" href="~/Dashboard/plugins/animo.js/animate-animo.min.css">
    <!-- Bootstrap Progressbar-->
    <link rel="stylesheet" type="text/css" href="~/Dashboard/plugins/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css">
    <!-- Toastr-->
    <link rel="stylesheet" type="text/css" href="~/Dashboard/plugins/toastr/toastr.min.css">
    <!-- SpinKit-->
    <link rel="stylesheet" type="text/css" href="~/Dashboard/plugins/SpinKit/css/spinners/7-three-bounce.css">
    <!-- Jvector Map-->
    <link rel="stylesheet" type="text/css" href="~/Dashboard/plugins/jvectormap/jquery-jvectormap-2.0.3.css">
    <!-- Morris Chart-->
    <link rel="stylesheet" type="text/css" href="~/Dashboard/plugins/morris.js/morris.css">
    <!-- Weather Icons-->
    <link rel="stylesheet" type="text/css" href="~/Dashboard/plugins/weather-icons/css/weather-icons-wind.min.css">
    <link rel="stylesheet" type="text/css" href="~/Dashboard/plugins/weather-icons/css/weather-icons.min.css">
    <!-- FullCalendar-->
    <link rel="stylesheet" type="text/css" href="~/Dashboard/plugins/fullcalendar/dist/fullcalendar.min.css">
    <link rel="stylesheet" type="text/css" href="~/Dashboard/plugins/fullcalendar/dist/fullcalendar.print.css" media="print">
    <!-- jQuery MiniColors-->
    <link rel="stylesheet" type="text/css" href="~/Dashboard/plugins/jquery-minicolors/jquery.minicolors.css">
    <!-- Bootstrap Date Range Picker-->
    <link rel="stylesheet" type="text/css" href="~/Dashboard/plugins/bootstrap-daterangepicker/daterangepicker.css">
    <!-- Primary Style-->
    <link rel="stylesheet" type="text/css" href="~/Dashboard/build/css/second-layout.css">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries-->
    <!-- WARNING: Respond.js doesn't work if you view the page via file://-->
    <!--[if lt IE 9]>
        <script type="text/javascript" src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script type="text/javascript" src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    @RenderSection("Css", false)
</head>
<body data-sidebar-color="sidebar-light" class="sidebar-light">
<!-- Header start-->
@Html.Partial("Partials/_NavigationPartial");
<!-- Header end-->
<div class="main-container">
    <!-- Main Sidebar start-->
    @Html.Partial("Partials/_LeftSideBarPartial");
    <!-- Main Sidebar end-->
    <div class="page-container">
        <div class="page-content container-fluid">
            @RenderBody()
        </div>
    </div>
    <!-- Right Sidebar start-->
    <!-- Right Sidebar start-->
    @*@Html.Partial("Partials/_RightSideBarPartial");*@

    <!-- Right Sidebar end-->
    <!-- Right Sidebar end-->
</div>
<!-- Demo Settings end-->
<!-- jQuery-->
<script type="text/javascript" src="~/Dashboard/plugins/jquery/dist/jquery.min.js"></script>
<!-- Bootstrap JavaScript-->
<script type="text/javascript" src="~/Dashboard/plugins/bootstrap/dist/js/bootstrap.min.js"></script>
<!-- Malihu Scrollbar-->
<script type="text/javascript" src="~/Dashboard/plugins/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.concat.min.js"></script>
<!-- Animo.js-->
<script type="text/javascript" src="~/Dashboard/plugins/animo.js/animo.min.js"></script>
<!-- Bootstrap Progressbar-->
<script type="text/javascript" src="~/Dashboard/plugins/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
<!-- jQuery Easy Pie Chart-->
<script type="text/javascript" src="~/Dashboard/plugins/jquery.easy-pie-chart/dist/jquery.easypiechart.min.js"></script>
<!-- Toastr-->
<script type="text/javascript" src="~/Dashboard/plugins/toastr/toastr.min.js"></script>
<!-- MomentJS-->
<script type="text/javascript" src="~/Dashboard/plugins/moment/min/moment.min.js"></script>
<!-- jQuery BlockUI-->
<script type="text/javascript" src="~/Dashboard/plugins/blockUI/jquery.blockUI.js"></script>
<!-- jQuery Counter Up-->
<script type="text/javascript" src="~/Dashboard/plugins/jquery-waypoints/waypoints.min.js"></script>
<script type="text/javascript" src="~/Dashboard/plugins/Counter-Up/jquery.counterup.min.js"></script>
<!-- Jvector Map-->
<script type="text/javascript" src="~/Dashboard/plugins/jvectormap/jquery-jvectormap-2.0.3.min.js"></script>
<script type="text/javascript" src="~/Dashboard/plugins/jvectormap/maps/jquery-jvectormap-world-mill.js"></script>
<!-- Flot Charts-->
<!--[if lte IE 8]>
    <script type="text/javascript" src="https://raw.githubusercontent.com/flot/flot/master/excanvas.min.js"></script>
<![endif]-->
<script type="text/javascript" src="~/Dashboard/plugins/flot/jquery.flot.js"></script>
<script type="text/javascript" src="~/Dashboard/plugins/flot/jquery.flot.resize.js"></script>
<script type="text/javascript" src="~/Dashboard/plugins/flot.curvedlines/curvedLines.js"></script>
<script type="text/javascript" src="~/Dashboard/plugins/flot.tooltip/js/jquery.flot.tooltip.min.js"></script>
<!-- Morris Chart-->
<script type="text/javascript" src="~/Dashboard/plugins/raphael/raphael-min.js"></script>
<script type="text/javascript" src="~/Dashboard/plugins/morris.js/morris.min.js"></script>
<!-- jQuery UI-->
<script type="text/javascript" src="~/Dashboard/plugins/jquery-ui/jquery-ui.min.js"></script>
<!-- FullCalendar-->
<script type="text/javascript" src="~/Dashboard/plugins/fullcalendar/dist/fullcalendar.min.js"></script>
<!-- jQuery MiniColors-->
<script type="text/javascript" src="~/Dashboard/plugins/jquery-minicolors/jquery.minicolors.min.js"></script>
<!-- Bootstrap Date Range Picker-->
<script type="text/javascript" src="~/Dashboard/plugins/bootstrap-daterangepicker/daterangepicker.js"></script>
<!-- Custom JS-->
<script type="text/javascript" src="~/Dashboard/build/js/second-layout/app.js"></script>
<script type="text/javascript" src="~/Dashboard/build/js/second-layout/demo.js"></script>
<script type="text/javascript" src="~/Dashboard/build/js/page-content/dashboard/index.js"></script>
</body>
</html>