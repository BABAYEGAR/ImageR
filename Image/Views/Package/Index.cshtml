
@using Image.Models.Entities
@model List<Package>
@{
    Layout = "Layout/_LayoutDashboard";
}
<table id="example-2" cellspacing="0" width="100%" class="table table-striped table-bordered">
    <thead>
    <tr>
        <th>Name</th>
        <th>Type</th>
        <th>Amount</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Type</td>
            <td>@item.Amount</td>
        </tr>
    }
    </tbody>
</table>